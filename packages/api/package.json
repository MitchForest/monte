{
  "name": "@monte/api",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./convex/_generated/*": {
      "import": "./convex/_generated/*",
      "default": "./convex/_generated/*"
    }
  },
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "lint": "echo \"No lint tasks for @monte/api\""
  },
  "dependencies": {
    "@monte/types": "workspace:*",
    "convex": "^1.27.5",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "tsup": "^8.5.0"
  }
}
